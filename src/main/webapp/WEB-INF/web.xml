<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <!--
    Servlet 全类名
    -->
    <servlet>
        <servlet-name>Demo8</servlet-name> <!-- 名称随意 -->
        <servlet-class>com.ServletDemo.Demo8</servlet-class> <!-- 类名为相应类的全类名 -->
    </servlet>

    <!--
    Servlet 访问路径
    -->
    <servlet-mapping>
        <servlet-name>Demo8</servlet-name> <!-- 名称为上边的定义名称 -->
        <url-pattern>/Demo8</url-pattern> <!-- 访问路径按需定义 -->
    </servlet-mapping>

</web-app>